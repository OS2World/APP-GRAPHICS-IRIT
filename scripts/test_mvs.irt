TimedInclude = function( FileName ):
    return = Time( true ):
    include(FileName);
TotalTime = 0.0;
PrintTime = procedure( FileName, BTime ): t:
    t = Time( false ):
    TotalTime = TotalTime + t - BTime:
    printf("************* Execution Time for %-13s is %-9.4f ******************\\n",
	   list( FileName, t - BTime ) );

